        <div class="content-wrapper">
            <div class="content-wrapper-before"></div>
            <div class="content-header row">
            </div>
            <div class="content-body">
               @RenderBody()
            </div>
        </div>
    </div>
      @await Html.PartialAsync("Partial/_FooterPartial")

    <!-- BEGIN VENDOR JS-->
    <script src="~/Dashboard/theme-assets/vendors/js/vendors.min.js" type="text/javascript"></script>
    <!-- BEGIN VENDOR JS-->
